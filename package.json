{
  "name": "retrievetokens",
  "version": "1.0.8",
  "description": "A simple package for interacting with web client storages like `localStorage`",
  "source": "src/index.ts",             
  "main": "dist/index.js",              
  "umd:main": "dist/index.umd.js",        
  "module": "dist/index.mjs",             
  "exports": {
    "types": "./dist/index.d.ts",         
    "require": "./dist/index.js",         
    "default": "./dist/index.modern.mjs"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "microbundle src/*.ts",
    "dev": "microbundle watch src/*.ts",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JUGG097/retrieveTokens.git"
  },
  "keywords": [
    "LocalStorage",
    "SessionStorage"
  ],
  "author": "Adeoluwa Adeboye <adeoluwaadeboye@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JUGG097/retrieveTokens/issues"
  },
  "homepage": "https://github.com/JUGG097/retrieveTokens#readme",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.1.0"
  }
}
